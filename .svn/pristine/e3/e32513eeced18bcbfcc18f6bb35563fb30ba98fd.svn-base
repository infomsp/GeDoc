<script>
    function chart_Tiempos(elem) {
        var id = elem.id;
        var title = $(elem).children(".dial-title").text();

        var UmbraMinimoTiempoEspera = parseFloat("@(Umbral.TEsperaMin)");
        var UmbraMaximoTiempoEspera = parseFloat("@(Umbral.TEsperaMax)");
        var UmbraMinimoTiempoAtencion = parseFloat("@(Umbral.TEsperaMin)");
        var UmbraMaximoTiempoAtencion = parseFloat("@(Umbral.TEsperaMax)");

        var realData = [0, 10, 15, 18, 32, 34, 50];
        var UmbralInicial = [30, 30, 30, 30, 30, 30, 30];
        var UmbralFinal = [70, 70, 70, 70, 70, 70, 70];
        var Escantidad = false;

        if (id === "divTiempoDeAtencion") {
            UmbralInicial = [UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion, UmbraMinimoTiempoAtencion];
            realData = [0, 12, 18, 26, 34, 45, 18];
            UmbralFinal = [UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion, UmbraMaximoTiempoAtencion];
        }
        else if (id === "divTiempoDeEspera") {
            UmbralInicial = [UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera, UmbraMinimoTiempoEspera];
            realData = [0, 12, 18, 26, 34, 45, 18];
            UmbralFinal = [UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera, UmbraMaximoTiempoEspera];
        }
        else if (id === "divCantidad") {
            UmbralInicial = [0, 20, 30, 35, 45, 50, 55];
            realData = [0, 12, 18, 26, 34, 45, 52, 65];
            Escantidad = true;
        }
        create_chart_Tiempos(title, UmbralInicial, realData, UmbralFinal, Escantidad);
    }

    function create_chart_Tiempos(title, planData, realData, UmbralFinal, Escantidad) {
        if (Escantidad) {
            chart = new Highcharts.Chart({
                chart: {
                    renderTo: 'chart-LineaTiempo',
                    height: 350
                },
                labels: {
                    items: [{
                        html: title,
                        style: {
                            left: '20px',
                            top: '14px',
                            color: 'gray',
                            "font-size": "16px"
                        }
                    }]
                },
                title: {
                    text: ''
                },
                yAxis: {
                    title: { text: "80% Atendidos" },
                    min: 0
                },
                xAxis: {
                    categories: ['29/01', '01/02', '02/02', '03/02', '04/02', '05/02', '10/02']
                },
                tooltip: {
                    formatter: function () {
                        var s;
                        if (this.point.name) { // the pie chart
                            s = '' +
                                this.point.name + ': ' + this.y;
                        } else {
                            s = '' +
                                this.series.name + " " + this.x + ': ' + this.y + "'";
                        }
                        return s;
                    }
                },
                series: [{
                    type: 'spline',
                    name: 'No Atendidos',
                    color: 'darkgray',
                    data: planData,
                    dashStyle: 'shortdot',
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'white'
                    }
                }, {
                    type: 'spline',
                    name: 'Atendidos',
                    color: 'darkGreen',
                    data: realData,
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'yellow'
                    }
                }]
            });
        }else{
            chart = new Highcharts.Chart({
                chart: {
                    renderTo: 'chart-LineaTiempo',
                    height: 350
                },
                labels: {
                    items: [{
                        html: title,
                        style: {
                            left: '20px',
                            top: '14px',
                            color: 'gray',
                            "font-size": "16px"
                        }
                    }]
                },
                title: {
                    text: ''
                },
                yAxis: {
                    title: { text: "Minutos" },
                    min: 0
                },
                xAxis: {
                    categories: ['29/01', '01/02', '02/02', '03/02', '04/02', '05/02', '10/02']
                },
                tooltip: {
                    formatter: function () {
                        var s;
                        if (this.point.name) { // the pie chart
                            s = '' +
                                this.point.name + ': ' + this.y;
                        } else {
                            s = '' +
                                this.series.name + " " + this.x + ': ' + this.y + "'";
                        }
                        return s;
                    }
                },
                series: [{
                    type: 'spline',
                    name: 'Umbra Inicial',
                    color: 'darkgray',
                    data: planData,
                    dashStyle: 'shortdot',
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'white'
                    }
                }, {
                    type: 'spline',
                    name: 'Valores',
                    color: 'darkGreen',
                    data: realData,
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'yellow'
                    }
                },
                {
                    type: 'spline',
                    name: 'Umbra Final',
                    color: 'darkgray',
                    data: UmbralFinal,
                    dashStyle: 'shortdot',
                    marker: {
                        lineWidth: 2,
                        lineColor: Highcharts.getOptions().colors[3],
                        fillColor: 'white'
                    }
                }]
            });
        }
    }

    function create_Tiempos() {
        divTiempoDeEspera = {
            id: "divTiempoDeEspera",
            title: "Tiempo de Espera",
            label: "Promedio",
            type: 2,
            value: 90,
            min: 0,
            max: 90,
            units: "'",
            decPlaces: 0,
            color: function () {
                if (Math.abs(this.value) >= 70) return Gauges.colors.danger;
                if (Math.abs(this.value) >= 30) return Gauges.colors.warning;
            }
        };
        Gauges.add(divTiempoDeEspera);

        Gauges.add({
            id: "divTiempoDeAtencion",
            title: "Tiempo de Atención",
            label: "Promedio",
            type: 2,
            value: 16,
            min: 0, max: 30,
            units: "'",
            decPlaces: 0,
            color: function () {
                if (Math.abs(this.value) >= 20) return Gauges.colors.danger;
                if (Math.abs(this.value) >= 10) return Gauges.colors.warning;
            }
        });

        Gauges.init();
        Gauges.animate(-20, 0.5);

        chart_Tiempos(document.getElementById("divTiempoDeEspera"));
    }

    $(document).ready(function () {
        create_Tiempos();
        $("#perId_Lista").prop("disabled", true);
    });
</script>