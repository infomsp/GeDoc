  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>OrigenDeDatos</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCon">
            <Value>=Parameters!idCon.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        catPersonaContratados.conId, catPersonaContratados.conFecha, catPersonaContratados.conApellidoyNombre, catPersonaContratados.tipoId, 
                         catPersonaContratados.conDNI, catPersonaContratados.conCUIL, catPersonaContratados.conDomicilio, catPersonaContratados.conTelefono, 
                         catPersonaContratados.conCelular, catPersonaContratados.conEmail, catPersonaContratados.profId, catPersonaContratados.espId, 
                         catPersonaContratados.conCargaHorariaSemanal, catPersonaContratados.conCuotas, catPersonaContratados.conMontoMensual, catPersonaContratados.repId, 
                         catPersonaContratados.conObservaciones, catPersonaContratados.conFechaBaja, catPersonaContratados.conMotivoBaja, catProfesion.profNombre
FROM            catPersonaContratados INNER JOIN
                         catProfesion ON catPersonaContratados.profId = catProfesion.profId
WHERE        (catPersonaContratados.conId = @idCon)</CommandText>
      </Query>
