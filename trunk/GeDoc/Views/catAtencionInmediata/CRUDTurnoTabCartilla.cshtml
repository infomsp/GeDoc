<style>
    #tabTurno-4 {
        padding: 0px;
        perspective: 800px;
        position: relative;
        height: 300px;
    }

     #Cartilla {
         width: 100%;
         height: 100%;
         position: absolute;
         transform-style: preserve-3d;
         transition: transform 1s;
     }

    #CartillaGridContainer * {
        backface-visibility: hidden;
    }

    #Cartilla>div {
        margin: 0;
        display: block;
        position: absolute;
        backface-visibility: hidden;
        width: 100%;
        height: 100%;
        transition: transform 1s;
        overflow: hidden;
    }

    #CartillaAddNew {
        transform: rotateY( 180deg );
    }

    #CartillaAddNew, #CartillaViewDetail {
        padding: 4px;
    }

    .btn-volver {
        display: inline-block;
        vertical-align: top;
        margin-right: 30px;
    }

    #CartillaViewDetail {
        transform: rotateX( 180deg );
    }

    #CartillaAddNew text,#CartillaViewDetail text {
        resize: none;
    }
     #CartillaAddNew checkbox,#CartillaViewDetail checkbox {
        resize: none;
    }
    .CartillaTableContainer {
        display: inline-block;
    }

    .CartillaTableContainer>table {
        margin: auto;
        width: 900px;
        table-layout: fixed;
    }

    .CartillaTableContainer>table tr:last-child {
        text-align: center;
    }

    checkbox.CartillaLastEntry:focus, checkbox.CartillaDetail:focus{
        -webkit-box-shadow: none;
        border: 1px solid rgba(0,0,0, 0.2);
    }
   checkbox.CartillaNewEntry:focus, checkbox.CartillaDetail:focus {
       -webkit-box-shadow: none;
        border: 1px solid rgba(0,0,0, 0.2);
       
   }
    #CartillaNewButtonContainer {
        /*display: inline-block;
        max-width: 80px;
        margin-left: 25px;
        vertical-align: top;
        text-align: center;*/
        position: absolute;
        top: 5px;
        right: 5px;
        max-width: 80px;
        text-align: center;
    }

    #CartillaNewButtonContainer>input {
        width: 62px;
    }

    #CartillaNewButtonContainer :first-child {
        margin-bottom: 10px;
    }

    .CartillaNewMessage {
        position: absolute;
        top: 0;
        left: 0;
        font-size: 40px;
        visibility: hidden;
        text-shadow: 10px 10px 20px grey;
        text-align: center;
    }

    #CartillaViewDetail div {
        display: inline-block;
        vertical-align: top;
        height: 97%;
    }

    #CartillaViewDetail>div:not(#CartillaViewDataContainer) {
        width: 9%;
    }

    #CartillaViewDataContainer {
        width: 80%;
    }

    #CartillaViewDataContainer>div {
        width: 80%;
    }

    #CartillaViewDataContainer table {
        width: 100%;
