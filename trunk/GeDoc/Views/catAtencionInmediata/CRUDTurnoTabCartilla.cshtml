    }

    /*#CartillaViewDataContainer textarea {
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
    }*/
   #CartillaViewDataContainer checkbox {
        width: 13px;
        height: 13px;
        margin: 0;
        padding: 0;
    }
    #CartillaViewButtonContainer input {
        width: 80%;
        margin: auto;
        margin-bottom: 5px;
    }

    #CartillaViewButtonContainer>div {
        text-align: center;
        height: 79%;
    }

    #CartillaViewButtonContainer>div:last-child {
        height: 19%;
    }

    #GridCartilla td, #CartillaViewDataContainer td {
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
    }

    .evd-tdiagprac {
        position: absolute;
        width: 47.7%!important;
        left: 10px;
        margin-top: 5px;
        height: 48%;
